

<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Download link</th>
      <th>Actions</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
      <tr>
        <td><%= @file_uploader.title %></td>
        <td><%= @file_uploader.description %></td>
        <td><%= @file_uploader.url %></td>
        <td><%= link_to 'Edit', edit_file_uploader_path(@file_uploader), class: "btn btn-success" %></td>
